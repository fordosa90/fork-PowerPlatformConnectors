                "designation": {
                  "$ref": "#/definitions/NEO_Designation"
                },
                "nasa_jpl_url": {
                  "$ref": "#/definitions/NEO_JPLURL"
                },
                "absolute_magnitude_h": {
                  "$ref": "#/definitions/NEO_AbsMagnitude"
                },
                "estimated_diameter": {
                  "$ref": "#/definitions/NEO_EstDiameter"
                },
                "is_potentially_hazardous_asteroid": {
                  "$ref": "#/definitions/NEO_IsHazardous"
                },
                "close_approach_data": {
                  "$ref": "#/definitions/NEO_CloseApproach"
                },
                "orbital_data": {
                  "$ref": "#/definitions/NEO_OrbitData"
                },
                "is_sentry_object": {
                  "$ref": "#/definitions/NEO_IsSentry"
                }
              }
            }
          }
        },
        "summary": "Lookup",
        "operationId": "Lookup",
        "description": "Lookup a specific Asteroid based on its NASA JPL small body (SPK-ID) ID.",
        "parameters": [
          {
            "name": "asteroid_id",
            "description": "Asteroid SPK-ID, which correlates to the NASA JPL small body.",
            "x-ms-summary": "Asteroid ID",
            "in": "path",
            "required": true,
            "type": "string",
            "x-ms-url-encoding": "single"
          }
        ]
      }
    }
  },
