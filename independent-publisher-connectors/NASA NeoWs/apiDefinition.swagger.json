{
  "swagger": "2.0",
  "info": {
    "title": "NASA NeoWs",
    "description": "NeoWs (Near Earth Object Web Service) is a web service for near earth Asteroid information: search for Asteroids based on their closest approach date to Earth, lookup a specific Asteroid with its NASA JPL small body id, as well as browse the overall data-set.",
    "version": "1.0",
    "contact": {
      "name": "Fördős András",
      "email": "fordosa90+ipc_nasaneo@gmail.com"
    }
  },
  "host": "api.nasa.gov",
  "basePath": "/neo/rest/",
  "schemes": [
    "https"
  ],
  "consumes": [],
  "produces": [],
  "paths": {
    "/v1/feed": {
      "get": {
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "links": {
                  "$ref": "#/definitions/LinksResponse"
                },
                "element_count": {
                  "type": "integer",
                  "format": "int32",
                  "description": "Number of results in current query.",
                  "title": "Element count"
                },
                "near_earth_objects": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "links": {
                          "$ref": "#/definitions/NEO_Links"
                        },
                        "id": {
                          "$ref": "#/definitions/NEO_ID"
                        },
                        "neo_reference_id": {
                          "$ref": "#/definitions/NEO_RefId"
                        },
                        "name": {
                          "$ref": "#/definitions/NEO_Name"
                        },
                        "nasa_jpl_url": {
                          "$ref": "#/definitions/NEO_JPLURL"
                        },
                        "absolute_magnitude_h": {
                          "$ref": "#/definitions/NEO_AbsMagnitude"
                        },
                        "estimated_diameter": {
                          "$ref": "#/definitions/NEO_EstDiameter"
                        },
                        "is_potentially_hazardous_asteroid": {
                          "$ref": "#/definitions/NEO_IsHazardous"
                        },
                        "close_approach_data": {
                          "$ref": "#/definitions/NEO_CloseApproach"
                        },
                        "is_sentry_object": {
                          "$ref": "#/definitions/NEO_IsSentry"
                        }
                      }
                    }
                  },
                  "description": "Collection of near earth objects.",
                  "title": "Near earth objects"
                }
              }
            }
          }
        },
        "summary": "Feed",
        "description": "Retrieve a list of Asteroids based on their closest approach date to Earth.",
        "operationId": "Feed",
        "parameters": [
          {
            "name": "start_date",
            "description": "Starting date for asteroid search, in YYYY-MM-DD format.",
            "x-ms-summary": "Start Date",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "end_date",
            "description": "Ending date for asteroid search, in YYYY-MM-DD format.",
            "x-ms-summary": "End Date",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ]
      }
    },
    "/v1/neo/browse": {
      "get": {
