                },
                "page": {
                  "type": "object",
                  "properties": {
                    "size": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Size of the page.",
                      "title": "Size"
                    },
                    "total_elements": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Number of total elements.",
                      "title": "Total elements"
                    },
                    "total_pages": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Number of total pages.",
                      "title": "Total pages"
                    },
                    "number": {
                      "type": "integer",
                      "format": "int32",
                      "description": "Page number.",
                      "title": "Number"
                    }
                  },
                  "description": "Page details.",
                  "title": "Page"
                },
                "near_earth_objects": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "links": {
                        "$ref": "#/definitions/NEO_Links"
                      },
                      "id": {
                        "$ref": "#/definitions/NEO_ID"
                      },
                      "neo_reference_id": {
                        "$ref": "#/definitions/NEO_RefId"
                      },
                      "name": {
                        "$ref": "#/definitions/NEO_Name"
                      },
                      "name_limited": {
                        "$ref": "#/definitions/NEO_NameLimited"
                      },
                      "designation": {
                        "$ref": "#/definitions/NEO_Designation"
                      },
                      "nasa_jpl_url": {
                        "$ref": "#/definitions/NEO_JPLURL"
                      },
                      "absolute_magnitude_h": {
                        "$ref": "#/definitions/NEO_AbsMagnitude"
                      },
                      "estimated_diameter": {
                        "$ref": "#/definitions/NEO_EstDiameter"
                      },
                      "is_potentially_hazardous_asteroid": {
                        "$ref": "#/definitions/NEO_IsHazardous"
                      },
                      "close_approach_data": {
                        "$ref": "#/definitions/NEO_CloseApproach"
                      },
                      "orbital_data": {
                        "$ref": "#/definitions/NEO_OrbitData"
                      },
                      "is_sentry_object": {
                        "$ref": "#/definitions/NEO_IsSentry"
                      }
                    }
                  },
                  "description": "Array of near earth objects.",
                  "title": "Near Earth Objects"
                }
              }
            }
          }
        },
        "summary": "Browse",
        "operationId": "Browse",
        "description": "Browse the overall Asteroid data-set.",
        "parameters": []
      }
    },
    "/v1/neo/{asteroid_id}": {
      "get": {
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "links": {
                  "$ref": "#/definitions/NEO_Links"
                },
                "id": {
                  "$ref": "#/definitions/NEO_ID"
                },
                "neo_reference_id": {
